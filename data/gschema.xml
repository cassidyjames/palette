<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/com/github/cassidyjames/palette/" id="com.github.cassidyjames.palette">
    <key name="window-x" type="i">
      <default>-1</default>
      <summary>Most recent x position</summary>
      <description>Most recent x position</description>
    </key>
    <key name="window-y" type="i">
      <default>-1</default>
      <summary>Most recent y position</summary>
      <description>Most recent y position</description>
    </key>
    <key name="developer-mode" type="b">
      <default>false</default>
      <summary>Developer mode</summary>
      <description>i.e. show Gtk.CSS color constants instead of hex values</description>
    </key>
  </schema>
</schemalist>

